#ifndef SED200_KEY_H
#define SED200_KEY_H

#define KEY_UP		(0xe9)
#define KEY_DOWN	(0xea)
#define KEY_RIGHT	(0xeb)
#define KEY_LEFT	(0xec)

#define KEY_BACKSPACE	0x08
#define KEY_TAB		0x9
#define KEY_ADD		L'+'
#define KEY_SUB		L'_'
#define KEY_QUIT	0x1
#define KEY_ENTER	0xd
#define KEY_DIT		L'.'
#define KEY_DELETE	0x7f
#define KEY_SALE	0xe4
#define KEY_PRINT	0xf4

#endif